{
  "name": "vite-modular-tailwindcss-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b && lit-analyzer",
    "test:dev": "cross-env PLAYGROUND_ENV=development pnpm playwright test",
    "test:preview": "cross-env PLAYGROUND_ENV=preview pnpm playwright test",
    "test": "pnpm test:dev && pnpm test:preview",
    "test:container": "cross-env PLAYWRIGHT_REMOTE_WS_ENDPOINT=ws://localhost:5375 PLAYWRIGHT_REMOTE_HOSTNAME=host.docker.internal pnpm test"
  },
  "dependencies": {
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@fontsource/inter": "^5.0.18",
    "@iconify-json/zondicons": "^1.1.8",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.8",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "lit-analyzer": "^2.0.3",
    "playwright": "^1.45.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-modular-tailwindcss": "workspace:*"
  }
}
